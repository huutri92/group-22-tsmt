@{
    ViewBag.Title = "Sắp xếp sinh viên lên xe";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@using (Html.BeginForm("AssignToCar", "Charity", FormMethod.Post))
{ 
    <input type="hidden" name="ceId" value="@ViewData["ceId"]" />
    <input type="submit" class="btn btn-info" value="Sắp xếp" />
    @Html.ActionLink("Quay lại", "DetailsCharityExam", new { ceId = @ViewData["ceId"] }, new { @class = "btn btn-info" })
}