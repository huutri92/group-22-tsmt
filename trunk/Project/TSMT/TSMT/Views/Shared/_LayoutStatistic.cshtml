@using TSMT.Models
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title - TSMT</title>
    <!-- CSS Global Compulsory-->
    <script type="text/javascript" src="~/Content/js/jquery.js"></script>
   @* <script type="text/javascript" src="~/Content/plugins/jquery-1.10.2.min.js"></script>*@
    <link rel="stylesheet" href="~/Content/plugins/bootstrap/css/bootstrap.css">
    <link rel="stylesheet" href="~/Content/css/style.css">
    <link rel="stylesheet" href="~/Content/css/headers/header1.css">
    <link rel="stylesheet" href="~/Content/css/responsive.css">
    <link rel="shortcut icon" href="~/Content/img/favicon.png">
    <!-- CSS Implementing Plugins -->
    <link rel="stylesheet" href="~/Content/plugins/font-awesome/css/font-awesome.css">
    <link rel="stylesheet" href="~/Content/plugins/flexslider/flexslider.css">
    <link rel="stylesheet" href="~/Content/plugins/bxslider/jquery.bxslider.css">
    <link rel="stylesheet" href="~/Content/plugins/fancybox/source/jquery.fancybox.css">
    <link rel="stylesheet" href="~/Content/plugins/revolution_slider/rs-plugin/css/settings.css" type="text/css" media="screen">
    <!-- CSS Theme -->
    <link rel="stylesheet" href="~/Content/css/pages/portfolio-v2.css">
    <link rel="stylesheet" href="~/Content/css/pages/page_one.css">
    <link href="~/Content/css/pages/page_search.css" rel="stylesheet" />

    <script src="~/Content/js/jquery-1.10.2.js"></script>
    <script src="~/Content/js/jquery.ui.effect.js"></script>
    <script src="~/Content/js/jquery.ui.effect-fold.js"></script>
   
    <script src="~/Scripts/ckeditor/ckeditor.js"></script>
    <script src="~/Content/js/jquery.ui.effect-fold.js"></script>
    <script src="~/Scripts/ckfinder/ckfinder.js"></script>

    <!-- CSS Theme -->
    @if (Session["acc"] != null)
    {

        int roleID = ((Account)Session["acc"]).RoleID;

        switch (roleID)
        {
            case 1: @*Candidate*@
        <link rel="stylesheet" href="~/Content/css/themes/blue.css">
        <link rel="stylesheet" href="~/Content/css/themes/headers/header1-blue.css">
                break;

            case 2: @*Charity*@
        <link rel="stylesheet" href="~/Content/css/themes/orange.css">
        <link rel="stylesheet" href="~/Content/css/themes/headers/header1-orange.css">
                break;

            case 3: @*Sponsor*@
        <link rel="stylesheet" href="~/Content/css/themes/default.css">
        <link rel="stylesheet" href="~/Content/css/headers/header1.css">
                break;

            case 4: @*Volunteer*@
        <link rel="stylesheet" href="~/Content/css/themes/orange.css">
        <link rel="stylesheet" href="~/Content/css/themes/headers/header1-orange.css">
                break;

            case 5: @*Admin*@
        <link rel="stylesheet" href="~/Content/css/themes/red.css">
        <link rel="stylesheet" href="~/Content/css/themes/headers/header1-red.css">
                break;

        }
    }
    else if (Session["acc"] == null)
    {
        <link rel="stylesheet" href="~/Content/css/themes/blue.css">
        <link rel="stylesheet" href="~/Content/css/themes/headers/header1-blue.css">

    }


    <!-- JS Global Compulsory -->

@*   <script type="text/javascript" src="~/Content/plugins/jquery-migrate-1.2.1.min.js"></script>*@
    <script type="text/javascript" src="~/Content/plugins/bootstrap/js/bootstrap.min.js"></script>
    <script type="text/javascript" src="~/Content/plugins/hover-dropdown.min.js"></script>
    <script type="text/javascript" src="~/Content/plugins/back-to-top.js"></script>
    <script type="text/javascript" src="~/Content/plugins/less-1.4.1.min.js"></script>
    <script type="text/javascript" src="~/Content/plugins/flexslider/jquery.flexslider-min.js"></script>
    <script type="text/javascript" src="~/Content/plugins/bxslider/jquery.bxslider.js"></script>
    <script type="text/javascript" src="~/Content/plugins/fancybox/source/jquery.fancybox.pack.js"></script>
    <script type="text/javascript" src="~/Content/plugins/revolution_slider/rs-plugin/js/jquery.themepunch.revolution.min.js"></script>
@*   <script type="text/javascript" src="~/Content/plugins/jquery.mixitup.min.js"></script>*@
    <script type="text/javascript" src="~/Content/js/app.js"></script>
    <script type="text/javascript" src="~/Content/js/pages/portfolio.js"></script>
    <script type="text/javascript" src="~/Content/js/pages/index.js"></script>



</head>
<body class="boxed-layout container">
    @* <!--=== Style Switcher ===-->
    <i class="style-switcher-btn icon-cogs hidden-xs"></i>
    <div class="style-switcher">
        <div class="theme-close"><i class="icon-remove"></i></div>
        <div class="theme-heading">Theme Colors</div>
        <ul class="list-unstyled">
            <li class="theme-default theme-active" data-style="default" data-header="light"></li>
            <li class="theme-blue" data-style="blue" data-header="light"></li>
            <li class="theme-orange" data-style="orange" data-header="light"></li>
            <li class="theme-red" data-style="red" data-header="light"></li>
            <li class="theme-light last" data-style="light" data-header="light"></li>
        </ul>
        <div style="margin-bottom: 18px;"></div>
        <div class="theme-heading">Layouts</div>
        <div class="text-center">
            <a href="#" class="btn-u btn-u-green btn-block active-switcher-btn wide-layout-btn">Wide</a>
            <a href="#" class="btn-u btn-u-green btn-block boxed-layout-btn">Boxed</a>
        </div>
    </div>
    <!--/style-switcher-->
    <!--=== End Style Switcher ===-->*@

   
    @Html.Partial("_HeaderPartial")

    @RenderSection("Breadcrumbs", required: false)

    <div class="container">
        <div class="row">

            @RenderBody()

        </div>
    </div>
    @Html.Partial("_FooterPartial")
    @Html.Partial("_CopyrightPartial")


    <script type="text/javascript">
        jQuery(document).ready(function () {
            App.init();
            Portfolio.init();
            App.initSliders();
            App.initFancybox();
            App.initBxSlider();
            Index.initRevolutionSlider();
        });
</script>
</body>
</html>
