@using TSMT.Models
@model IEnumerable<DetailsScheduleExam>
@{
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h2 class="title_of_page">Manage Schedule Exam</h2>
<div class="inner-page-content inner-careers">
	<table class="table table-hover table-bordered ">
		<thead>
			<tr>
				<th>#</th>
				<th>Giờ bắt đầu</th>
				<th>Giờ kết thúc</th>
				<th>Actions</th>
			</tr>
		</thead>
		<tbody>
			@{
				int i = 1;
                foreach (DetailsScheduleExam item in Model)
				{
				<tr>
					<td>@i</td>
					<td>@item.BeginHour</td>
					<td>@item.EndHour</td>
					<td>
						@Html.ActionLink("Edit", "EditScheduleExam", new { scheduleExamId = @item.DetailsScheduleExamID })
					</td>
				</tr>
					i = i + 1;
				}
			}
		</tbody>
	</table>
</div>
<button class="btn btn-info" onclick="window.history.back(-1);">Back</button>