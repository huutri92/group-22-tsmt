@{
    ViewBag.Title = "Edit Venue";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@using TSMT.Models
@model TSMT.Models.Venue

<h2 class="title_of_page">@ViewBag.Title</h2>

@using (Html.BeginForm("EditVenue", "Admin", FormMethod.Post))
{
    <input type="hidden" name="veId" value="@Model.VenueID"/>
    <div class="inner-page-content inner-careers">
        <table class="table table-hover table-bordered ">
            <tr>
                <td>Địa chỉ</td>
                <td>
                    <input type="text" name="Address" value="@Model.Address"/></td>
            </tr>
        </table>
        <input type="submit" value="Submit" class="btn btn-info" />
        <input type="reset" value="Reset" class="btn btn-info" />
        <input type="button" value="Cancel" class="btn btn-info" onclick="location.href = '/admin/ManageVenue/' + @Model.UniversitiesExamination.UniExamID"/>
    </div>
}