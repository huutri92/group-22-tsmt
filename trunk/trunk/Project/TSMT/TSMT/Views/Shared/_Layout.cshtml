@using TSMT.Models

<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title - TSMT</title>

    <link href='http://fonts.googleapis.com/css?family=Open+Sans:400italic,400,600' rel='stylesheet' type='text/css'>
    <link href='http://fonts.googleapis.com/css?family=Open+Sans+Condensed:300,700' rel='stylesheet' type='text/css'>

    <link href="/Content/css/bootstrap.min.css" rel="stylesheet">
    <link href="/Content/css/settings.css" rel="stylesheet">
    <link href="/Content/css/animate.min.css" rel="stylesheet">
    <link href="/Content/css/prettyPhoto.css" rel="stylesheet">
    <link href="/Content/css/font-awesome.min.css" rel="stylesheet">
    <link href="/Content/css/style.css" rel="stylesheet">

    <link rel="shortcut icon" href="#">

    <link href="~/Content/Admin/css/bootstrap.min.css" rel="stylesheet">
    <link href="~/Content/Admin/css/jquery-ui.css" rel="stylesheet">
    <link href="~/Content/Admin/css/jquery.gritter.css" rel="stylesheet">
    <link href="~/Content/Admin/css/bootstrap-switch.css" rel="stylesheet">
    <link href="~/Content/Admin/css/jquery.dataTables.css" rel="stylesheet">
    <link href="~/Content/Admin/css/rateit.css" rel="stylesheet">
    <link href="~/Content/Admin/css/prettyPhoto.css" rel="stylesheet">
    <link href="~/Content/Admin/css/fullcalendar.css" rel="stylesheet">
    <link href="~/Content/Admin/css/font-awesome.min.css" rel="stylesheet">
    <link href="~/Content/Admin/css/style.css" rel="stylesheet"> 


</head>
<body>
    <div class="out-container">
        @Html.Partial("_HeaderPartial")
        <div class="outer">
            <div class="sidebar">
                <div class="logo">
                    <h1><a href="/">TSMT</a></h1>
                </div>
                @Html.Partial("_LoginPartial")

                <div class="sidebar-search">
                    <form class="form-inline" role="form">
                        <div class="input-group">
                            <input type="text" class="form-control" id="s" placeholder="Type Here to Search...">
                            <span class="input-group-btn">
                                <button class="btn btn-default" type="submit"><i class="icon-search"></i></button>
                            </span>
                        </div>
                    </form>
                </div>

                @Html.Partial("_NavigationPartial")
            </div>
            <div class="mainbar">
                @RenderBody()

                @Html.Partial("_FooterPartial")
            </div>
            <div class="clearfix"></div>
        </div>
    </div>

    <span class="totop"><a href="#"><i class="icon-chevron-up"></i></a></span>
    <script src="Content/js/jquery.js"></script>
    <script src="Content/js/bootstrap.min.js"></script>
    <script src="Content/js/jquery.themepunch.revolution.min.js"></script>
    <script src="Content/js/jquery.themepunch.plugins.min.js"></script>
    <script src="Content/js/waypoints.min.js"></script>
    <script type="text/javascript" src="Content/js/isotope.js"></script>
    <script type="text/javascript" src="Content/js/jquery.prettyPhoto.js"></script>
    <script src="Content/js/respond.min.js"></script>
    <script src="Content/js/html5shiv.js"></script>
    <script src="Content/js/custom.js"></script>
</body>
</html>
