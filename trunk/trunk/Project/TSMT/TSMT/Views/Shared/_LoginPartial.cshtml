@if (Session["acc"] != null)
{ 
    <div class="sidebar-buttons text-center">
        <div class="btn-group">
            <a href="@Url.Action("Logout", "Home")" class="btn btn-black btn-xs"><i class="icon-lock"></i></a>
            <a href="@Url.Action("Logout", "Home")" class="btn btn-danger btn-xs">Sign Out</a>
        </div>
    </div>
}
else
{
    <div class="sidebar-buttons text-center">
        <div class="btn-group">
            <a href="@Url.Action("Entrance", "Home")" class="btn btn-black btn-xs"><i class="icon-user"></i></a>
            <a href="@Url.Action("Entrance", "Home")" class="btn btn-danger btn-xs">Sign In</a>
        </div>
        <div class="btn-group">
            <a href="@Url.Action("Registration", "Home")" class="btn btn-black btn-xs"><i class="icon-lock"></i></a>
            <a href="@Url.Action("Registration", "Home")" class="btn btn-danger btn-xs">Sign Up</a>
        </div>
    </div>
}
